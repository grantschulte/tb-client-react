{
  "autoInstall": true,
  "base": "src/ui",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": false,
  "version": "2.4.2",
  "components": [
    "reset",
    "site",
    "button",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "form",
    "menu",
    "message",
    "card",
    "item",
    "checkbox",
    "dropdown",
    "modal",
    "popup",
    "search",
    "transition"
  ]
}